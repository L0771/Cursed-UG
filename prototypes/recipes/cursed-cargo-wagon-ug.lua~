
local obj = util.table.deepcopy(data.raw["recipe"]["cargo-wagon"])
obj.name = "cursed-cargo-wagon-ug"
obj.results = {}
obj.results[1] = {}
obj.results[1].type = "item"
obj.results[1].name = "cursed-cargo-wagon-ug"
obj.results[1].amount = 1
data.raw[obj.type][obj.name] = obj


local obj = util.table.deepcopy(data.raw["recipe"]["cargo-wagon"])
obj.name = "cursed-cargo-wagon"
for _,ingredient in pairs(obj.ingredients) do
   ingredient[2] = math.floor(ingredient[2] * 2)
end
obj.results = {}
obj.results[1] = {}
obj.results[1].type = "item"
obj.results[1].name = "cursed-cargo-wagon"
obj.results[1].amount = 1
data.raw[obj.type][obj.name] = obj
